    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "eslint": "^5.2.0",
    "istanbul": "^0.4.5",
    "jsdom": "11.11.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "react-addons-css-transition-group": "^15.6.2",
    "sinon": "^6.1.4"
